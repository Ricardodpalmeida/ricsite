---
// This is a catch-all page that will handle any attempts to access content directly
// For static builds, we need to define getStaticPaths

export function getStaticPaths() {
  return [
    { params: { rest: undefined } },
    { params: { rest: 'profile' } },
    { params: { rest: 'profile/data' } },
    { params: { rest: 'profile/data.json' } }
  ];
}

// Immediately redirect to 404
return Astro.redirect('/404');
--- 