{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "astro"
  },
  "exclude": [
    "node_modules",
    "dist",
    ".astro",
    "src/pages/*/music.astro",
    "src/pages/*/podcast.astro"
  ]
}
